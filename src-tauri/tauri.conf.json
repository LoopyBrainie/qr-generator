{
"build": {
  "beforeBuildCommand": "",
  "beforeDevCommand": "",
  "devPath": "../src",
  "distDir": "../src"
},
  "package": {
    "productName": "签到码生成器",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.qr-generator.dev",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "签到码生成器",
        "width": 800,
        "height": 700
      }
    ]
  }
}
