{
"build": {
  "beforeBuildCommand": "",
  "beforeDevCommand": "",
  "devPath": "../src",
  "distDir": "../src"
},
  "package": {
    "productName": "签到码生成器",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "identifier": "com.loopybrainie.qrgenerator",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.ico"
      ],
      "resources": [
        "../LICENSE",
        "../README.md",
        "../Software_Usage_Declaration.html",
        "../Software_Usage_Declaration_zh.html"
      ],
      "windows": {
        "wix": {
          "language": "zh-CN"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "签到码生成器",
        "width": 800,
        "height": 700
      }
    ]
  }
}
